<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/commons_constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context="it.polimi.mathkit.fragments.CommonsFragment">
    
	<Button
		android:id="@+id/calcCommButton"
		
		android:layout_width="150.0dp"
		
		android:layout_height="40.0dp"
		
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
    	
    	
		
		app:layout_constraintTop_toBottomOf="@+id/lcmLabel"
		app:layout_constraintBottom_toBottomOf="parent"
		
		android:layout_marginTop="0dp"
		android:layout_marginBottom="0dp"
		
		
		android:layout_marginStart="0dp"
		android:layout_marginEnd="0dp"
		
		android:paddingBottom="0dp"
		android:paddingRight="0dp"
		android:paddingLeft="0dp"
		android:paddingTop="0dp"

        
        android:textColor="#ffffff"
        
        android:background="@drawable/calccommbutton_background"
		style="?android:attr/borderlessButtonStyle"
        android:textAllCaps="false"
        android:text="Calculate"/>        
	<TextView
		android:id="@+id/inputsLabel"
		
		
		android:layout_width="88.0dp"
		
		android:layout_height="36.0dp"
		
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
    	
    	
		
		
		app:layout_constraintVertical_chainStyle="spread"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintBottom_toTopOf="@+id/firstLabel"
		
		android:layout_marginTop="0dp"
		android:layout_marginBottom="0dp"
		
		
		android:layout_marginStart="0dp"
		android:layout_marginEnd="0dp"
		
		android:paddingBottom="0dp"
		android:paddingRight="0dp"
		android:paddingLeft="0dp"
		android:paddingTop="0dp"

        
        android:textStyle="bold"
        android:text="Inputs"
        android:gravity="center|center_vertical"
        android:textColor="#000000"
        android:textSize="24dp"/>
	<TextView
		android:id="@+id/firstLabel"
		
		
		android:layout_width="0dp"
		
		android:layout_height="36.0dp"
		app:layout_constraintWidth_percent="0.5"
		
		app:layout_constraintStart_toStartOf="parent"
    	
    	
		
		app:layout_constraintTop_toBottomOf="@+id/inputsLabel"
		app:layout_constraintBottom_toTopOf="@+id/secondLabel"
		
		android:layout_marginTop="0dp"
		android:layout_marginBottom="0dp"
		
		
		android:layout_marginStart="0dp"
		android:layout_marginEnd="0dp"
		
		android:paddingBottom="0dp"
		android:paddingRight="0dp"
		android:paddingLeft="0dp"
		android:paddingTop="0dp"

        
        android:textStyle="bold"
        android:text="First number ="
        android:gravity="right|center_vertical"
        android:textColor="#000000"
        android:textSize="18dp"/>
	<TextView
		android:id="@+id/secondLabel"
		
		
		android:layout_width="0dp"
		
		android:layout_height="36.0dp"
		app:layout_constraintWidth_percent="0.5"
		
		app:layout_constraintStart_toStartOf="parent"
    	
    	
		
		app:layout_constraintTop_toBottomOf="@+id/firstLabel"
		app:layout_constraintBottom_toTopOf="@+id/resultsCommonsLabel"
		
		android:layout_marginTop="0dp"
		android:layout_marginBottom="0dp"
		
		
		android:layout_marginStart="0dp"
		android:layout_marginEnd="0dp"
		
		android:paddingBottom="0dp"
		android:paddingRight="0dp"
		android:paddingLeft="0dp"
		android:paddingTop="0dp"

        
        android:textStyle="bold"
        android:text="Second number ="
        android:gravity="right|center_vertical"
        android:textColor="#000000"
        android:textSize="18dp"/>
	<TextView
		android:id="@+id/resultsCommonsLabel"
		
		
		android:layout_width="88.0dp"
		
		android:layout_height="36.0dp"
		
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
    	
    	
		
		app:layout_constraintTop_toBottomOf="@+id/secondLabel"
		app:layout_constraintBottom_toTopOf="@+id/gcfLabel"
		
		android:layout_marginTop="0dp"
		android:layout_marginBottom="0dp"
		
		
		android:layout_marginStart="0dp"
		android:layout_marginEnd="0dp"
		
		android:paddingBottom="0dp"
		android:paddingRight="0dp"
		android:paddingLeft="0dp"
		android:paddingTop="0dp"

        
        android:textStyle="bold"
        android:text="Results"
        android:gravity="center|center_vertical"
        android:textColor="#000000"
        android:textSize="24dp"/>
	<TextView
		android:id="@+id/gcfLabel"
		
		
		android:layout_width="0dp"
		
		android:layout_height="36.0dp"
		app:layout_constraintWidth_percent="0.5"
		
		app:layout_constraintStart_toStartOf="parent"
    	
    	
		
		app:layout_constraintTop_toBottomOf="@+id/resultsCommonsLabel"
		app:layout_constraintBottom_toTopOf="@+id/lcmLabel"
		
		android:layout_marginTop="0dp"
		android:layout_marginBottom="0dp"
		
		
		android:layout_marginStart="0dp"
		android:layout_marginEnd="0dp"
		
		android:paddingBottom="0dp"
		android:paddingRight="0dp"
		android:paddingLeft="0dp"
		android:paddingTop="0dp"

        
        android:textStyle="bold"
        android:text="GCF ="
        android:gravity="right|center_vertical"
        android:textColor="#000000"
        android:textSize="20dp"/>
	<TextView
		android:id="@+id/lcmLabel"
		
		
		android:layout_width="0dp"
		
		android:layout_height="36.0dp"
		app:layout_constraintWidth_percent="0.5"
		
		app:layout_constraintStart_toStartOf="parent"
    	
    	
		
		app:layout_constraintTop_toBottomOf="@+id/gcfLabel"
		app:layout_constraintBottom_toTopOf="@+id/calcCommButton"
		
		android:layout_marginTop="0dp"
		android:layout_marginBottom="0dp"
		
		
		android:layout_marginStart="0dp"
		android:layout_marginEnd="0dp"
		
		android:paddingBottom="0dp"
		android:paddingRight="0dp"
		android:paddingLeft="0dp"
		android:paddingTop="0dp"

        
        android:textStyle="bold"
        android:text="LCM ="
        android:gravity="right|center_vertical"
        android:textColor="#000000"
        android:textSize="20dp"/>
	<TextView
		android:id="@+id/gcfResultLabel"
		
		android:layout_width="88.0dp"
		android:layout_height="36.0dp"
		
		app:layout_constraintTop_toTopOf="@+id/gcfLabel"
		app:layout_constraintStart_toEndOf="@+id/gcfLabel"
    	
		
		android:layout_marginStart="10dp"
		
		android:layout_marginTop="0dp"
		android:layout_marginEnd="0dp"
		android:layout_marginBottom="0dp"
		
		android:paddingBottom="0dp"
		android:paddingRight="0dp"
		android:paddingLeft="0dp"
		android:paddingTop="0dp"

        android:text="result"
        android:gravity="left|center_vertical"
        android:textColor="#000000"
        android:textSize="20dp"/>
	<TextView
		android:id="@+id/lcmResultLabel"
		
		android:layout_width="88.0dp"
		android:layout_height="36.0dp"
		
		app:layout_constraintTop_toTopOf="@+id/lcmLabel"
		app:layout_constraintStart_toEndOf="@+id/lcmLabel"
    	
		
		android:layout_marginStart="10dp"
		
		android:layout_marginTop="0dp"
		android:layout_marginEnd="0dp"
		android:layout_marginBottom="0dp"
		
		android:paddingBottom="0dp"
		android:paddingRight="0dp"
		android:paddingLeft="0dp"
		android:paddingTop="0dp"

        android:text="result"
        android:gravity="left|center_vertical"
        android:textColor="#000000"
        android:textSize="20dp"/>
	<android.support.v7.widget.AppCompatEditText
		android:id="@+id/firstEditText"
		
		android:layout_width="88.0dp"
		android:layout_height="36.0dp"
		
		app:layout_constraintTop_toTopOf="@+id/firstLabel"
		app:layout_constraintStart_toEndOf="@+id/firstLabel"
    	
		
		android:layout_marginStart="10dp"
		
		android:layout_marginTop="0dp"
		android:layout_marginEnd="0dp"
		android:layout_marginBottom="0dp"
		
		android:paddingBottom="0dp"
		android:paddingRight="0dp"
		android:paddingLeft="0dp"
		android:paddingTop="0dp"
       
        android:text=""
        android:hint="first" 
        android:textColor="#000000"
        android:textSize="18dp"/>
	<android.support.v7.widget.AppCompatEditText
		android:id="@+id/secondEditText"
		
		android:layout_width="88.0dp"
		android:layout_height="36.0dp"
		
		app:layout_constraintTop_toTopOf="@+id/secondLabel"
		app:layout_constraintStart_toEndOf="@+id/secondLabel"
    	
		
		android:layout_marginStart="10dp"
		
		android:layout_marginTop="0dp"
		android:layout_marginEnd="0dp"
		android:layout_marginBottom="0dp"
		
		android:paddingBottom="0dp"
		android:paddingRight="0dp"
		android:paddingLeft="0dp"
		android:paddingTop="0dp"
       
        android:text=""
        android:hint="second" 
        android:textColor="#000000"
        android:textSize="18dp"/>

	
</android.support.constraint.ConstraintLayout>
